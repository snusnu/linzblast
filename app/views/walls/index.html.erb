<h3>all walls</h3>
 
<table class="resources">
	<thead>
	  <tr>
	    <th>name</th>
	    <th>description</th>
	  </tr>
	</thead>
	<tfoot>
		<tr>
			<td></td>
		</tr>
	</tfoot>
	<tbody>
	<% @walls.each do |wall| %>
	  <tr>
	    <td><%=h wall.name %></td>
	    <td><%=h wall.description %></td>
	  </tr>
	<% end %>
	</tbody>
</table>